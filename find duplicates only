<?php
// function to find the duplicates from array;

$array = [1,1,2,3,3,4,5,6,7,7,8,8,9,9,1,0,10,12,13,13,14,14,15,15,16];

$length = count($array);

//starting the loop counter;
for ($i=0;$i<$length;$i++){
    //starting the other looo counter
    for($j=$i+1;$j<$length;$j++){
        
        // checking for array i == arrayj
        
        if($array[$i] == $array[$j]){
           echo $array[$j] . ", ";
        }
    }
}
?>
